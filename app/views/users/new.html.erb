<h1>Sign Up</h1>
<nav>
  <li><%=link_to "Home", root_path%></li>
  <li><%=link_to "Sign Up", new_user_path%></li>
  <li>Log In</li>
  <li><%=link_to "Search", search_path%></li>
</nav>
<br>

<%= form_for @user do |f| %>
  <%=f.label :fname, "First Name" %>
  <%=f.text_field :fname %>
  <br>
  <%=f.label :lname, "Last Name" %>
  <%=f.text_field :lname %>
  <br>
  <%=f.label :email, "Email" %>
  <%=f.text_field :email %>
  <br>
  <%=f.label :password, "Password" %>
  <%=f.password_field :password %>
  <br> 
  <%=f.submit :create %> 
  <!-- OR you can use a string bc submit doesnt take a form -->
<% end %>